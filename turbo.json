{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["dist/**"],
      "dependsOn": ["^build"]
    },
    "dev": {
      "dependsOn": ["@ruijadom/prisma#build"],
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "start": {
      "cache": false
    }
  }
}
